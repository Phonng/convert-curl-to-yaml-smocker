{
  "extends": ["eslint:recommended", "next/core-web-vitals"],

  "rules": {
    "no-nested-ternary": "warn",

    "require-await": "off",
    "camelcase": ["error", { "allow": ["^unstable_"] }],
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_" }],
    "no-warning-comments": 0,
    "prettier/prettier": "error"
  },
  "globals": {}
}
